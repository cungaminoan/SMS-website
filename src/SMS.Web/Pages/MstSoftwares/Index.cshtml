@page
@using SMS.Localization
@using SMS.Web.Pages.MstSoftwares
@using Microsoft.Extensions.Localization
@model IndexModel
@inject IStringLocalizer<SMSResource> L
@section scripts
    {
    <abp-script src="/Pages/MstSoftwares/Index.js" />
}

<abp-card>
    <abp-card-header>
        <h2>@L["Quản lý thông tin phần mềm"]</h2>
        <abp-row>
            <abp-column size-md="_6">
            </abp-column>
            <abp-column size-md="_6" class="text-end">
                <abp-button id="NewMstSoftwareButton"
                            text="@L["Thêm mới"].Value"
                            icon="plus"
                            button-type="Primary" />
            </abp-column>
        </abp-row>
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="MstSoftwaresTable"></abp-table>
    </abp-card-body>
</abp-card>
